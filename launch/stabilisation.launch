  <node name="positionStabiliser" pkg="tricopter" type="position_stabilisation.py" required="true" output="screen"/> 
  <!-- <node name="velocityStabiliser" pkg="tricopter" type="velocity_stabilisation.py" required="true" output="screen"/>  -->
  <include file="$(find delta_2)/launch/run_manipulator.launch"/>
  <param name="mavros/local_position/tf/send" value="True" />
</launch>